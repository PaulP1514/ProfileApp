{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": {
      "type": "object",
      "properties": {
        "birthday": {
          "type": "string",
          "format": "date-time"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "major": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "year": {
          "type": "string"
        }
      },
      "required": ["birthday", "image", "major", "name", "year"]
    }
  }
  